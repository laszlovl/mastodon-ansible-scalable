global:
  scrape_interval:     15s
  evaluation_interval: 15s

scrape_configs:
  - job_name: 'json'
    file_sd_configs:
      - files:
        - 'targets.json'

    relabel_configs:
      - source_labels: [ __address__ ]
        target_label: instance
        regex: '(.+?)(:\d+)?'
        replacement: '${1}'
